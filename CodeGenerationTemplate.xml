<Class name="TestClass">
	<annotation name="Data"></annotation>
	<annotation name="MappedSuperClass"></annotation>
	<annotation name="Inheritance">
		<property name="strategy">InheritanceType.TABLE_PER_CLASS</property>
	</annotation>
	<field name="id" type="Long">
		<annotation name="Id"></annotation>
		<annotation name="GeneratedValue">
			<property name="strategy">GenerationType.AUTO</property>			
		</annotation>
		<annotation name="Column">
			<property name="name">id</property>			
		</annotation>
	</field>
	<field name="createdDate" type="Date">
		<annotation name="CreatedDate"></annotation>
		<annotation name="TemporalType">
			<property name="TemporalType.TIMESTAMP"></property>			
		</annotation>
	</field>
	<field name="deletedDate" type="Date">
		<annotation name="DeletedDate"></annotation>
		<annotation name="TemporalType">
			<property name="TemporalType.TIMESTAMP"></property>			
		</annotation>
	</field>
	<field name="active" type="Boolean">
		<annotation name="Column">
			<property name="name">active</property>			
		</annotation>
	</field>
</Class>